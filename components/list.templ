package components

import (
	"github.com/AslanSN/CurriculumVitae/helpers"
)

templ List(label string, slice []string) {
	<label class="text-xl underline" for={ helpers.Identity(label) }>{ label }</label>
	<ul class="list-disc list-inside">
		for _, item := range slice {
			@ListItem(item)
		}
	</ul>
}
