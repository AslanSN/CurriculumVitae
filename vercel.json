{
	"public": "true",
	"rewrites": [
		{
			"source": "/assets/(.*)",
			"destination": "/assets/$1"
		},
		{
			"source": "/assets/icons/(.*)",
			"destination": "/assets/icons/$1"
		},
		{
			"source": "/assets/images/(.*)",
			"destination": "/assets/images/$1"
		},
		{
			"source": "/assets/js/(.*)",
			"destination": "/assets/js/$1"
		},
		{
			"source": "/(.*)",
			"destination": "/vercel/handler.go"
		}
	],
	"functions": {
		"vercel/handler.go": {
			"runtime": "vercel-go@0.5.2"
		}
	}
}